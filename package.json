    "express": "^5.2.1",
    "lucide-react": "^0.562.0",
    "mongoose": "^9.1.3",
    "next": "15.5.7",
    "posthog-js": "^1.270.1",
    "react": "^19",
    "react-dom": "^19",
    "share-api-polyfill": "^1.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "oxlint": "^1.12.0",
    "postcss": "^8",
    "supabase": "2.34.3",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "unrs-resolver"
